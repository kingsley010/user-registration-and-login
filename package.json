{
  "name": "mern-note-list",
  "version": "1.0.0",
  "description": "A note list built with the MERN stack",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node start",
    "server": "nodemon start",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/perfect090/Note-List-MERN.git"
  },
  "engines": {
    "node": "10.15.0",
    "npm": "6.4.1"
  },
  "keywords": [
    "node",
    "heroku",
    "express",
    "babel"
  ],
  "author": "Kingsley Obioha",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.9.6",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.9.6",
    "@babel/register": "^7.9.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "concurrently": "^5.2.0",
    "config": "^3.3.6",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.8",
    "nodemon": "^2.0.12",
    "regenerator-runtime": "^0.13.9"
  },
  "devDependencies": {
    "morgan": "^1.10.0"
  }
}
